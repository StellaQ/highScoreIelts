/**index.wxss**/

.containerThreeparts {
  padding: 30px 0;
  margin-bottom: 30px;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-direction: row;
  background-color: #def0e5;
}

.containerRecord{
  display: flex;
  padding: 0 20px;
  /* width: 100%; */
  justify-content: space-between;
}
.containerLeftRecord {
  width: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.containerNotebook{
  width: 50%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.containerNotebook image{
  width: 100px;
  height: 100px;
}
